html { height: 100%; }

body {
  height: 100%;
  margin: 0;
  padding: 10px 0 0 0;
  color: #383D44;
  font: 76%/1.4 Helvetica, Arial, sans-serif;
  text-align: center;
  background: url(../images/oneone/header-bg.png) repeat-x left top;
}
* { font-family: Helvetica, Arial, sans-serif; }

a { color: #1F7083; }
div#main a {
  text-decoration: none;
  border-bottom: 1px dotted;
}
div#main a:hover {
  color: #383D44;
  border-bottom: 1px solid;
}
a.destroy { color: #E64D3C; font-weight: bold; }
a.highlighted {
  display: block;
  float: left;
  margin-top: 10px;
  padding: 5px 5px 3px 5px;
  line-height: 1;
  font-weight: bold;
  border: 1px solid #1F7083 !important;
}

a img { border: none; }

li.unread { font-weight: bold; }
li.private {
  padding-left: 17px;
  background: url(../images/oneone/lock.gif) no-repeat left center;
}

p.intro {
  margin: 5px 0 20px 0;
  font-size: 117%;
}

h1, h2, h3, h4, h5, h6 { margin: 0; padding: 0; }
h3 { color: #2B97B2; }

div#main ul.quick-list li { margin-bottom: 3px; }
  div#main ul.quick-list li a {  }
  div#main a.remove,
  div#main a.add { float: right; margin: 0; line-height: 1; border: none; }
    div#main a.remove img {  }
  
div#main a.date,
div#main a.date:hover,
div#main div.date {
  display: block;
	float: left;
	width: 48px;
	height: 48px;
	margin-right: 10px;
	padding: 13px 4px 0 0;
	color: #fff;
	text-align: center;
	line-height: 1;
	font-weight: bold;
	border: none;
	background: url(../images/oneone/date-green.png) no-repeat left top;
}
div#main li.below a.date,
div#main li.above a.date,
div#main li.below div.date,
div#main li.above div.date { background-image: url(../images/oneone/date-yellow.png); }
div#main li.missing a.date,
div#main li.missing div.date { background-image: url(../images/oneone/date-red.png); }
	div#main a.date span,
	div#main div.date span { display: block; padding: 2px 0; }
	div#main a.date span.month,
	div#main div.date span.month {
	  color: #777D80;
		text-transform: uppercase;
	}
	div#main a.date span.day,
	div#main div.date span.day {
		padding: 5px 0 3px 0;
		font-size: 150%;
		background: none;
	}

dt { font-weight: bold; }


/* PROGRESS
######################################## */

div.progress {
  position: relative;
  height: 18px;
  margin: 0;
  padding: 5px;
  text-align: center;
  border: 1px solid #ccc;
  background: url(../images/progress-wrapper-bg.png) repeat-x left top;
}
  div.progress span.bar {
    position: absolute;
    left: 5px;
    top: 5px;
    display: block;
    height: 16px;
    border: 1px solid #65B542;
    background: #77CB66 url(../images/progress-bg.png) repeat-x left top;
  }
  div.progress.yellow span.bar { border-color: #E4A809; background-image: url(../images/progress-bg-yellow.png); background-color: #F2C207; }
  div.progress.red span.bar { border-color: #DE2929; background-image: url(../images/progress-bg-red.png); background-color: #E15353; }
  div.progress label {
    position: relative;
    z-index: 10;
    color: #333;
    font-size: 11px;
    font-weight: bold;
    line-height: 1.8;
  }


/* FORMS
######################################## */

form { margin: 0; }

form p.field {
  margin: 0 0 15px 0;
  padding: 0;
}
  form p.field label {
    display: block;
    font-weight: bold;
    line-height: 1.2;
  }
    form p label span {
      color: #666;
      font-weight: normal;
    }
  form p.field textarea,
  form p.field select,
  form p.field input[type=text],
  form p.field input[type=password] { width: 300px; font-size: 108%; }
  form p.field.double input[type=text] { width: 145px; margin-right: 5px; }
  form p small {
    display: block;
    font-size: 92%;
  }
form p.submit { margin: 0; padding: 0; }

div.photo-upload {
  width: 350px;
  margin-top: 10px;
  padding: 15px;
  background: #E3F4F9;
}
  div.photo-upload input.submit {
    margin-top: 15px;
  }

div#entry-photo {
  margin: 25px 0;
  padding: 15px;
  background: #E3F4F9;
}
  div#entry-photo label {
    float: left;
    margin-bottom: 10px;
    font-weight: bold;
  }
  div#entry-photo a { line-height: 1; font-weight: bold; }
  div#entry-photo img {
    float: right;
    border-color: #2B97B2;
  }


/* LISTS
######################################## */

ul.list {
  margin: 0;
  padding: 0;
  list-style: none;
}
  ul.list div.thumb {
    float: left;
    overflow: hidden;
    width: 70px;
    height: 70px;
    margin-right: 10px;
  }
    ul.list div.thumb img { width: 70px; }


ul.list.journals {}
  ul.list.journals li {
    position: relative;
    margin: 0 0 20px 0;
    padding: 0;
  }
  ul.list.journals li.private {
    background: url(../images/oneone/lock.gif) no-repeat right 4px;
  }
    ul.list.journals div.thumb {
      width: 44px;
      height: 44px;
      padding: 1px;
      border: 5px solid #2B97B2;
    }
      ul.list.journals div.thumb a { border-color: #fff; }
      ul.list.journals div.thumb img { width: 44px; height: 44px; }
    ul.list.journals li div.info { margin-left: 66px }
    ul.list.journals li h2 {
      margin: 0 0 10px 0;
      padding: 0;
      line-height: 1;
      font-size: larger;
    }
      ul.list.journals li h2 span {
        color: #666;
        font-size: smaller;
        text-transform: none;
      }
        ul.list.journals li h2 span a { color: #666; }
        ul.list.journals li h2 span a:hover { text-decoration: underline; }
    ul.list.journals li p.groups,
    ul.list.journals li p.date-range {
      display: none;
      margin: 4px 0 5px 0;
      padding: 0 0 0 15px;
      line-height: 1.0;
      color: #666;
      font-size: 92%;
      background: url(../images/calendar.gif) no-repeat left center;
    }
/*    ul.list.journals li div.progress { width: 436px; float: left; } */
    ul.list.journals li a.more {
      display: block;
      float: right;
      width: 28px;
      height: 28px;
      border: 1px solid #1F7083;
    }
    
    ul.list.journals li a.edit {
      position: absolute;
      right: 0;
      top: 18px;
      line-height: 1;
    }
    ul.list.journals li:first-child a.edit { top: 3px; }

ul.list.groups li h2 { margin-bottom: 8px; }
ul.list.groups li p { margin: 0; }
  
ul.list.entries {}
  ul.list.entries li {
    margin: 0 0 20px 0;
    padding: 0;
  }
  ul.list.entries li.draft {
    background: url(../images/new/draft-bg.png) no-repeat left top;
  }
/*  ul.list.entries li.below,
  ul.list.entries li.missing,
  ul.list.entries li.above { background: #E64D3C; }*/
    ul.list.entries li div.info { margin-left: 65px; }
    ul.list.entries li p {
      margin: 3px 0 0 0;
      font-size: 117%;
    }
      ul.list.entries li p a { white-space: nowrap; }
    ul.list.entries li img {
      float: right;
      width: 80px;
      height: 80px;
      margin-left: 10px;
      padding: 1px;
      border: 4px solid #ccc;
    }
    div#body ul.list.entries li p.meta {
      margin: 10px 0 0 0;
      padding: 0 0 0 21px;
      font-size: 12px;
      color: #666;
    }
      div#body ul.list.entries li p.meta a { color: #666; }
    
ul.list.users li {
  float: left;
  width: 50%;
  margin-bottom: 20px;
}
  ul.list.users li img { float: left; margin-right: 10px; }
  ul.list.users li h3 { margin: 0 0 5px 0; line-height: 1; }
  ul.list.users li p { margin: 0; }


/* TABS
######################################## */

ul.tabs {
  display: none;
  float: right;
  margin: 0 0 20px 0;
  padding: 0 20px 0 0;
  list-style: none;
  line-height: 1;
  font-size: 54%;
}
  ul.tabs li {
    margin-left: 5px;
    float: left;
    padding: 9px;
    border: 1px solid #B0A58A;
    background: #E6E3D7;
  }
    ul.tabs li a { color: #666; }
    ul.tabs li a:hover { color: #326EA1; }
  ul.tabs li.active {
    border-bottom: 1px solid #F0EDE3;
    background: url(../images/tab-active-bg.png) repeat-x left top;
  }
    ul.tabs li.active a { font-weight: bold; color: #e40; }

ul.subtabs {
  display: none;
  margin: 0 0 20px 0;
  padding: 0;
  list-style: none;
  line-height: 1;
}
  ul.subtabs li {
    float: left;
    margin-right: 5px;
  }
    ul.subtabs li a {
      padding: 2px 5px;
      color: #666;
      text-transform: uppercase;
      font-size: 92%;
      background: #E6E3D7;
    }
    ul.subtabs li a:hover { color: #326EA1; }
    ul.subtabs li.active a {
      color: #e40;
      font-weight: bold;
    }


/* THUMBNAILS
######################################## */

ul.thumbnails {
  margin: 0;
  padding: 0;
  list-style: none;
}
  ul.thumbnails li {
    position: relative;
    float: left;
    margin: 0 15px 15px 0 !important;
    padding: 0 !important;
    text-align: center;
    border: none !important;
  }
  ul.thumbnails li.right { margin-right: 0 !important; }
  
    img.thumbnail,
    ul.thumbnails li img {
      width: 42px;
      height: 42px;
      padding: 1px;
      border: 4px solid #fff;
    }

    img.thumbnail.small { width: 32px; height: 32px; }
    img.thumbnail.large { width: 80px; height: 80px; padding: 1px; border: 4px solid #ccc; }
    

/* PAGINATOR
######################################## */    
div#paginator {
  clear: both;
  margin-top: 10px;
}
  div#paginator a {
    display: block;
    float: left;
    margin-right: 5px;
    padding: 5px 5px 3px 5px;
    line-height: 1;
    border: 1px solid #1F7083 !important;
  }
  div#paginator span {
    display: block;
    float: left;
    margin-right: 5px;
    padding: 5px 5px 3px 5px;
    color: #fff;
    line-height: 1;
    font-weight: bold;
    border: 1px solid #2B97B2 !important;
    background: #2B97B2;
  }
  div#paginator span.disabled {
    color: #888;
    font-weight: normal;
    border: 1px solid #888 !important;
    background: none;
  }
  div#paginator span.break {
    color: #000;
    font-weight: normal;
    border: none !important;
    background: none;
  }   
    
/* LAYOUT
######################################## */

